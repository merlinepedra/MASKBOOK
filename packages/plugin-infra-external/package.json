{
  "name": "@masknet/plugin-infra-external",
  "description": "Runner of external plugins",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    "./background-worker": {
      "types": "./dist/entry-background-worker.d.ts",
      "webpack": "./src/entry-background-worker.ts",
      "default": "./dist/entry-background-worker.js"
    },
    "./dashboard": {
      "types": "./dist/entry-dashboard.d.ts",
      "webpack": "./src/entry-dashboard.ts",
      "default": "./dist/entry-dashboard.js"
    },
    "./content-script": {
      "types": "./dist/entry-content-script.d.ts",
      "webpack": "./src/entry-content-script.ts",
      "default": "./dist/entry-content-script.js"
    }
  },
  "typesVersions": {
    "*": {
      "background-worker": [
        "./dist/entry-background-worker.d.ts"
      ],
      "dashboard": [
        "./dist/entry-dashboard.d.ts"
      ],
      "content-script": [
        "./dist/entry-content-script.d.ts"
      ]
    }
  },
  "dependencies": {
    "@dimensiondev/holoflows-kit": "^0.9.0-20220520093249-0870919",
    "@masknet/compartment": "^0.1.1",
    "@masknet/icons": "workspace:*",
    "@masknet/plugin-infra": "workspace:*",
    "@masknet/shared-base": "workspace:*",
    "@masknet/theme": "workspace:*",
    "@masknet/typed-message": "workspace:*",
    "async-call-rpc": "^6.1.0"
  },
  "peerDependencies": {
    "react-i18next": "^11.15.3"
  }
}
